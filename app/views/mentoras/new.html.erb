<h1> Nova Mentora</h1>
<%= form_with(model: @mentora, local:true) do |form| %> 
    <div>
        <%= form.label :nome %>
        <%= form.text_field :nome %>
   </div>
   <div>
        <%= form.label :email %>
        <%= form.text_field :email %>
   </div>
   <div>
        <%= form.label :telefone %>
        <%= form.text_field :telefone %>
   </div>
   <div>
        <%= form.label :habilidades %>
        <%= form.text_field :habilidades %>
   </div>
   <div>
        <%= form.submit "Cadastrar Mentora" %>
   </div>
<% end %>
   

        
